<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<fx:root type="VBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-background-radius: 50; -fx-background-color: lightgray;"
          xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="playerLabel" text="playerLabel">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
    </children>
</fx:root>