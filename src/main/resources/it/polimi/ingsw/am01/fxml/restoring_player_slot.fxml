<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<fx:root alignment="CENTER" prefHeight="200.0" prefWidth="100.0" type="VBox" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Circle fx:id="pawn" fill="DODGERBLUE" radius="30.0" stroke="BLACK" strokeType="INSIDE"/>
        <Label fx:id="playerLabel" text="playerName">
            <font>
                <Font name="System Bold" size="16.0"/>
            </font>
        </Label>
        <Label fx:id="connectionLabel" text="connected" textFill="#4ec936"/>
    </children>
</fx:root>
