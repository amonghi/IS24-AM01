<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0"
         prefWidth="400.0" style="-fx-border-width: 1; -fx-border-color: black; -fx-border-radius: 10;"
         type="AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER" prefHeight="0.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="gameIDLabel" prefHeight="0.0" prefWidth="100.0" text="Game 0" VBox.vgrow="NEVER">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <Label fx:id="playersLabel" prefHeight="0.0" prefWidth="100.0" text="1/4 players">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
        <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="0.0" prefWidth="100.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="joinButton" mnemonicParsing="false" onAction="#join" style="-fx-background-color: BLACK;"
                        text="Join" textFill="WHITE">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</fx:root>
